{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      login: '',\n      mail: '',\n      password: '',\n      confirm_password: '',\n      title: 'Регистрация',\n      subtitle: 'Вход',\n      auth: 'Зарегистрироваться',\n      errors: []\n    };\n  },\n  methods: {\n    authUser() {\n      if (this.auth == 'Зарегистрироваться') {\n        const data = {\n          username: this.login,\n          email: this.mail,\n          password: this.password\n        };\n      } else if (this.auth == 'Войти') {\n        console.log(\"Goodbay\");\n        const data = {\n          email: this.mail,\n          password: this.password\n        };\n        axios.post('http://localhost:5000/auth/login', data).then(response => {\n          this.mail = '', this.password = '', console.log(response);\n        }).catch(error => {\n          this.errors = error.response.data.errors;\n          console.log(this.errors);\n        });\n      }\n    },\n    AuthOrReg() {\n      if (this.subtitle == \"Вход\") {\n        this.title = \"Вход\";\n        this.subtitle = \"Регистрация\";\n        this.auth = \"Войти\";\n      } else if (this.subtitle == \"Регистрация\") {\n        this.title = \"Регистрация\";\n        this.subtitle = \"Вход\";\n        this.auth = \"Зарегистрироваться\";\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","login","mail","password","confirm_password","title","subtitle","auth","errors","methods","authUser","username","email","console","log","post","then","response","catch","error","AuthOrReg"],"sources":["C:\\Users\\Sergey\\Documents\\GitHub\\task-manager\\client\\src\\components\\Auth\\AuthorizationClient.vue"],"sourcesContent":["<template>\r\n  <div class=\"section\">\r\n    <div class=\"section__item\">\r\n      <div class=\"section__logo\">\r\n        <img class=\"logo\" src=\"../../assets/images/auth/logo.svg\" alt=\"logo\">\r\n        <h3 class=\"section__text\">Task Unity Tech</h3>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"section__item\">\r\n      <div class=\"section__auth\">\r\n        <h3 class=\"section__title\">{{ title }}</h3>\r\n        <button @click=\"AuthOrReg\" class=\"section__subtitle\">{{ subtitle }}</button>\r\n      </div>\r\n      \r\n    </div>\r\n    <div class=\"section__item\">\r\n      <form action=\"\" method=\"post\">\r\n        <input v-if=\"this.title == 'Регистрация' \" class=\"section__input\" type=\"text\" placeholder=\"Имя пользователя\" v-model=\"login\">\r\n\r\n        <input class=\"section__input\" type=\"email\" placeholder=\"E-mail\" v-model=\"mail\">\r\n        <input class=\"section__input\" type=\"password\" placeholder=\"Пароль\" v-model=\"password\">\r\n        <input v-if=\"this.title == 'Регистрация'\" class=\"section__input\" type=\"password\" placeholder=\"Подтверждение пароля\" v-model=\"confirm_password\">\r\n        <a v-if=\"this.title == 'Вход'\" class=\"section__forget\" href=\"#\">Забыли пароль?</a>\r\n      </form>\r\n    </div>\r\n\r\n    <div class=\"section__item\">\r\n      \r\n      <button type=\"submit\" class=\"section__button\" @click=\"authUser\">{{ auth }}</button>\r\n      <div class=\"section__text-or\">\r\n        <span>или</span>\r\n      </div>\r\n\r\n      <div class=\"section__bt-container\">\r\n        <a href=\"#\"><img src=\"../../assets/images/auth/google.svg\"></a>\r\n        <a href=\"#\"><img src=\"../../assets/images/auth/github.svg\"></a>\r\n        <a href=\"#\"> <img src=\"../../assets/images/auth/facebook.svg\"></a>\r\n      </div>\r\n\r\n    </div>\r\n</div>\r\n  <!-- <img :src=\"flag == true ? require('../../assets/images/auth/ile.png') : require('../../assets/images/auth/ile2.png')\"> -->\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  data(){\r\n    return { \r\n      login: '', \r\n      mail: '', \r\n      password: '', \r\n      confirm_password: '', \r\n\r\n      title: 'Регистрация', \r\n      subtitle: 'Вход', \r\n      auth: 'Зарегистрироваться',\r\n      errors: []\r\n    }\r\n  }, \r\n\r\n  methods: {\r\n  \r\n    authUser() {\r\n\r\n      if(this.auth == 'Зарегистрироваться'){\r\n        const data = {\r\n        username: this.login,\r\n        email: this.mail,\r\n        password: this.password,\r\n          } \r\n\r\n      \r\n      }\r\n      else if (this.auth == 'Войти') {\r\n\r\n        console.log(\"Goodbay\")\r\n        const data = {\r\n          email: this.mail,\r\n          password: this.password,\r\n        }\r\n\r\n        axios.post('http://localhost:5000/auth/login', data)\r\n        .then((response) => {\r\n            this.mail = '', \r\n            this.password = '', \r\n\r\n            console.log(response)\r\n        })\r\n\r\n        .catch((error) => {\r\n          this.errors = error.response.data.errors\r\n          console.log(this.errors)\r\n        })   \r\n      }\r\n      \r\n    },     \r\n\r\n    AuthOrReg(){\r\n      if(this.subtitle == \"Вход\"){\r\n        this.title = \"Вход\"\r\n        this.subtitle = \"Регистрация\"\r\n        this.auth = \"Войти\"\r\n\r\n      }\r\n\r\n      else if(this.subtitle == \"Регистрация\"){\r\n        this.title = \"Регистрация\"\r\n        this.subtitle = \"Вход\"\r\n        this.auth = \"Зарегистрироваться\"\r\n      }\r\n    }\r\n    }\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n    \r\n\r\n</script>\r\n"],"mappings":"AA+CA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAIA,CAAA,EAAE;IACJ,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE,EAAE;MACZC,gBAAgB,EAAE,EAAE;MAEpBC,KAAK,EAAE,aAAa;MACpBC,QAAQ,EAAE,MAAM;MAChBC,IAAI,EAAE,oBAAoB;MAC1BC,MAAM,EAAE;IACV;EACF,CAAC;EAEDC,OAAO,EAAE;IAEPC,QAAQA,CAAA,EAAG;MAET,IAAG,IAAI,CAACH,IAAG,IAAK,oBAAoB,EAAC;QACnC,MAAMP,IAAG,GAAI;UACbW,QAAQ,EAAE,IAAI,CAACV,KAAK;UACpBW,KAAK,EAAE,IAAI,CAACV,IAAI;UAChBC,QAAQ,EAAE,IAAI,CAACA;QACb;MAGJ,OACK,IAAI,IAAI,CAACI,IAAG,IAAK,OAAO,EAAE;QAE7BM,OAAO,CAACC,GAAG,CAAC,SAAS;QACrB,MAAMd,IAAG,GAAI;UACXY,KAAK,EAAE,IAAI,CAACV,IAAI;UAChBC,
      <div class=\"section__bt-container\">\r\n        <a href=\"#\"><img src=\"../../assets/images/auth/google.svg\"></a>\r\n        <a href=\"#\"><img src=\"../../assets/images/auth/github.svg\"></a>\r\n        <a href=\"#\"> <img src=\"../../assets/images/auth/facebook.svg\"></a>\r\n      </div>\r\n\r\n    </div>\r\n</div>\r\n\r\n</template>\r\n\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  data(){\r\n    return { \r\n      login: '', \r\n      mail: '', \r\n      password: '', \r\n      confirm_password: '', \r\n\r\n\r\n      title: 'Регистрация', \r\n      subtitle: 'Вход', \r\n      auth: 'Зарегистрироваться',\r\n      errors: [],\r\n\r\n      show: true\r\n    }\r\n  }, \r\n\r\n  methods: {\r\n  \r\n    authUser() {\r\n\r\n\r\n      if(this.password !== this.confirm_password){\r\n        this.errors.push(\"Пароли не совпадают\")\r\n        return \r\n      }\r\n\r\n      let data = {}\r\n      let type = ''\r\n      if(this.auth == 'Зарегистрироваться'){\r\n        data = {\r\n        username: this.login,\r\n        email: this.mail,\r\n        password: this.password,\r\n          } \r\n        \r\n        type = 'registration'\r\n      }\r\n      else if (this.auth == 'Войти') {\r\n        data = {\r\n          email: this.mail,\r\n          password: this.password,\r\n        }\r\n\r\n        type = 'login'\r\n      }\r\n\r\n\r\n      axios.post(`http://localhost:5000/auth/${type}`, data)\r\n        .then((response) => {\r\n            this.login = ''\r\n            this.mail = '', \r\n            this.password = '', \r\n            this.confirm_password = ''\r\n\r\n            \r\n            console.log(response)\r\n        })\r\n\r\n        .catch((error) => {\r\n          this.errors = error.response.data.errors\r\n          console.log(error)\r\n        })   \r\n    },     \r\n\r\n    AuthOrReg(){\r\n\r\n      this.errors = []\r\n      this.login = ''\r\n      this.mail = '', \r\n      this.password = '', \r\n      this.confirm_password = ''\r\n\r\n      if(this.subtitle == \"Вход\"){\r\n        this.title = \"Вход\"\r\n        this.subtitle = \"Регистрация\"\r\n        this.auth = \"Войти\"\r\n\r\n      }\r\n\r\n      else if(this.subtitle == \"Регистрация\"){\r\n        this.title = \"Регистрация\"\r\n        this.subtitle = \"Вход\"\r\n        this.auth = \"Зарегистрироваться\"\r\n      }\r\n    }\r\n    }\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n    \r\n\r\n</script>\r\n\r\n\r\n<style>\r\n/* .input-enter-active, .input-leave-active {\r\n  transition: opacity 500ms;\r\n}\r\n.input-enter, .input-leave-to {\r\n  opacity: 0;\r\n} */\r\n\r\n\r\n/* \r\n.section__auth:focus  .section__title\r\n {     transform: translate(80px,0);\r\n\r\n\r\n }  \r\n\r\n .section__auth:focus .section__subtitle {\r\n    transform: translate(-250px,0);\r\n}  \r\n */\r\n\r\n\r\n .section__auth:focus  .section__input\r\n {     width: 50%;\r\n\r\n\r\n }  \r\n\r\n\r\n\r\n\r\n\r\n\r\n</style>"],"mappings":";AAsDA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAIA,CAAA,EAAE;IACJ,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE,EAAE;MACZC,gBAAgB,EAAE,EAAE;MAGpBC,KAAK,EAAE,aAAa;MACpBC,QAAQ,EAAE,MAAM;MAChBC,IAAI,EAAE,oBAAoB;MAC1BC,MAAM,EAAE,EAAE;MAEVC,IAAI,EAAE;IACR;EACF,CAAC;EAEDC,OAAO,EAAE;IAEPC,QAAQA,CAAA,EAAG;MAGT,IAAG,IAAI,CAACR,QAAO,KAAM,IAAI,CAACC,gBAAgB,EAAC;QACzC,IAAI,CAACI,MAAM,CAACI,IAAI,CAAC,qBAAqB;QACtC;MACF;MAEA,IAAIZ,IAAG,GAAI,CAAC;MACZ,IAAIa,IAAG,GAAI,EAAC;MACZ,IAAG,IAAI,CAACN,IAAG,IAAK,oBAAoB,EAAC;QACnCP,IAAG,GAAI;UACPc,QAAQ,EAAE,IAAI,CAACb,KAAK;UACpBc,KAAK,EAAE,IAAI,CAACb,IAAI;UAChBC,QAAQ,EAAE,IAAI,CAACA;QACb;QAEFU,IAAG,GAAI,cAAa;MACtB,OACK,IAAI,IAAI,CAACN,IAAG,IAAK,OAAO,EAAE;QAC7BP,IAAG,GAAI;UACLe,KAAK,EAAE,IAAI,CAACb,IAAI;UAChBC,QAAQ,EAAE,IAAI,CAACA;QACjB;QAEAU,IAAG,GAAI,OAAM;MACf;MAGAd,KAAK,CAACiB,IAAI,CAAE,8BAA6BH,IAAK,EAAC,EAAEb,IAAI,EAClDiB,IAAI,CAAEC,QAAQ,IAAK;QAChB,IAAI,CAACjB,KAAI,GAAI,EAAC;QACd,IAAI,CAACC,IAAG,GAAI,EAAE,EACd,IAAI,CAACC,QAAO,GAAI,EAAE,EAClB,IAAI,CAACC,gBAAe,GAAI,EAAC;QAGzBe
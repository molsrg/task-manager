 <div>\r\n                <input\r\n                    ref=\"taskDate\"\r\n                    class=\"form-input\"\r\n                    type=\"text\"\r\n                    placeholder=\"Дата выполнения\"\r\n                    v-model=\"taskStartDate\"\r\n                    :min=\"this.formattedDate\"\r\n                    onfocus=\"(this.type='date')\" onblur=\"(this.type='text')\"\r\n                />\r\n\r\n                <span\r\n                    class=\"invalid-span\"\r\n                    v-if=\"v$.taskStartDate.$invalid && v$.taskStartDate.$dirty\"\r\n                    >Введите дату</span\r\n                >\r\n            </div>\r\n            <div class=\"form-input_time-container\">\r\n                <div>\r\n                    <input\r\n                        id=\"start-time\"\r\n                        ref=\"taskStartTime\"\r\n                        class=\"form-input-time\"\r\n                        type=\"text\"\r\n                        placeholder=\"Время начала\"\r\n                        onfocus=\"(this.type='time')\" onblur=\"(this.type='text')\"\r\n                    />\r\n                    <span\r\n                        class=\"invalid-span\"\r\n                        v-if=\"\r\n                            v$.taskStartTime.$invalid && v$.taskStartDate.$dirty\r\n                        \"\r\n                        >Введите время</span\r\n                    >\r\n                </div>\r\n\r\n                <div>\r\n                    <input\r\n                        id=\"end-time\"\r\n                        ref=\"taskEndTime\"\r\n                        class=\"form-input-time\"\r\n                        type=\"text\"\r\n                        placeholder=\"Время окончания\"\r\n                        onfocus=\"(this.type='time')\" onblur=\"(this.type='text')\"\r\n                    />\r\n                    <span\r\n                        class=\"invalid-span\"\r\n                        v-if=\"\r\n         